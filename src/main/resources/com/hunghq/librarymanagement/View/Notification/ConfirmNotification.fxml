            minHeight="150.0" minWidth="200.0" prefHeight="150.0" prefWidth="350.0">
    <children>
        <Label fx:id="lbl_message" layoutX="42.0" layoutY="18.0" text="Text label"/>
        <Button fx:id="btn_confirm" layoutX="264.0" layoutY="105.0" mnemonicParsing="false" text="Confirm">
        </Button>
        <Button fx:id="btn_cancel" layoutX="195.0" layoutY="105.0" mnemonicParsing="false" text="Cancel">
        </Button>
    </children>
